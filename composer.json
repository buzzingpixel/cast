{
    "name": "buzzingpixel/cast",
    "description": "Podcasting CMS addon",
    "version": "1.0.0",
    "license": "proprietary",
    "homepage": "https://www.buzzingpixel.com",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "league/flysystem": "^1.0",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^3.8",
        "symfony/filesystem": "^4.2",
        "thecodingmachine/safe": "^0.1.15",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.5",
        "phpstan/phpstan-deprecation-rules": "^0.11.0",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.2",
        "thecodingmachine/phpstan-safe-rule": "^0.1.3"
    },
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Cast\\": "src"
        },
        "classmap": [
            "src/ExpressionEngine/ft.cast_audio.php",
            "src/ExpressionEngine/ft.cast_episode_number.php",
            "src/ExpressionEngine/mcp.cast.php",
            "src/ExpressionEngine/mod.cast.php",
            "src/ExpressionEngine/upd.cast.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "EllisLab\\": "work/testing/ee3/system/ee/EllisLab"
        },
        "classmap": [
            "work/testing/ee3/system/ee/legacy/fieldtypes/EE_Fieldtype.php",
            "work/testing/ee3/system/ee/legacy/core/Loader.php",
            "work/testing/ee3/system/ee/legacy/core/Lang.php",
            "work/testing/ee3/system/ee/legacy/core/Config.php",
            "work/testing/ee3/system/ee/legacy/libraries/Cp.php",
            "work/testing/ee3/system/ee/legacy/database/DB_forge.php",
            "work/testing/ee3/system/ee/legacy/database/drivers/mysqli/mysqli_driver.php",
            "work/testing/ee3/system/ee/legacy/core/Input.php",
            "work/testing/ee3/system/ee/legacy/database/DB_result.php",
            "work/testing/ee3/system/ee/legacy/libraries/Template.php",
            "work/testing/ee3/system/ee/legacy/core/URI.php",
            "work/testing/ee3/system/ee/legacy/core/Input.php"
        ]
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "handle": "cast",
        "systemPath": "src/ExpressionEngine"
    }
}
